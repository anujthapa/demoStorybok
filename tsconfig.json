{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "noResolve": false,
      "noImplicitAny": false,
      "removeComments": true,
      "sourceMap": true,
      "allowJs": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule":true,
      "baseUrl": ".",
      "paths": {
        "common/*": ["src/common/*"],
        "assets/*": ["src/assets/*"]
      }
    },
    "include": [
        "src"
    ],
    "exclude": [
      "./node_modules/**/*"
    ]
}
